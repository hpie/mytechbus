<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="10dp">
    <!--<ImageView-->
    <!--android:src="@drawable/header_logo"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="64dp"-->
    <!--android:scaleType="center"-->
    <!--android:background="#FFFFBB33"-->
    <!--android:contentDescription="@string/app_name" />-->
    <LinearLayout
        android:id="@+id/row1"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_marginBottom="5dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/proptFullLabel"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="FT"
            android:textSize="18sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:text=" : "
            android:textSize="18sp" />

        <TextView
            android:id="@+id/proptFullRate"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:text="60"
            android:textAlignment="textEnd"
            android:textSize="18sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:text="*"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/proptFullPassengers"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:paddingLeft="4dp"
            android:text="1"
            android:textAlignment="textEnd"
            android:textSize="18sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text=" = "
            android:textSize="18sp" />

        <TextView
            android:id="@+id/proptFullTotal"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="2"
            android:paddingEnd="10dp"
            android:text="120"
            android:textAlignment="textEnd"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/row2"
        android:layout_below="@+id/row1"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_marginBottom="5dp"
        android:orientation="horizontal"
        >

        <TextView
            android:id="@+id/proptHalfLabel"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="HT"
            android:textSize="18sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:text=" : "
            android:textSize="18sp" />

        <TextView
            android:id="@+id/proptHalfRate"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:text="60"
            android:textAlignment="textEnd"
            android:textSize="18sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:text="*"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/proptHalfPassengers"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:paddingLeft="4dp"
            android:text="2"
            android:textAlignment="textEnd"
            android:textSize="18sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text=" = "
            android:textSize="18sp" />

        <TextView
            android:id="@+id/proptHalfTotal"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="2"
            android:paddingEnd="10dp"
            android:text="120"
            android:textAlignment="textEnd"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/row3"
        android:layout_below="@+id/row2"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_marginBottom="5dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/proptLuggageLabel"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="LT"
            android:textSize="18sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:text=" : "
            android:textSize="18sp" />

        <TextView
            android:id="@+id/proptLuggageRate"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:text="60"
            android:textAlignment="textEnd"
            android:textSize="18sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:text="*"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/proptLuggage"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:paddingLeft="4dp"
            android:text="2"
            android:textAlignment="textEnd"
            android:textSize="18sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text=" = "
            android:textSize="18sp" />

        <TextView
            android:id="@+id/proptLuggageTotal"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="2"
            android:paddingEnd="10dp"
            android:text="120"
            android:textAlignment="textEnd"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/row4"
        android:layout_below="@+id/row3"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_marginBottom="5dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/proptDiscountLabel"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="DT"
            android:textSize="18sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:text=" : "
            android:textSize="18sp" />


        <TextView
            android:id="@+id/proptTotalForDiscount"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:paddingLeft="4dp"
            android:text="2"
            android:textAlignment="textEnd"
            android:textSize="18sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:text="*"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/proptDiscountRate"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:text="60"
            android:textAlignment="textEnd"
            android:textSize="18sp" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text=" = "
            android:textSize="18sp" />

        <TextView
            android:id="@+id/proptDiscountApplied"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="2"
            android:paddingEnd="10dp"
            android:text="120"
            android:textAlignment="textEnd"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/row5"
        android:layout_below="@+id/row4"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_marginBottom="5dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/proptTicketLabel"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="2"
            android:text="Total"
            android:textSize="22dp" />

        <!--<TextView-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="40dp"-->
        <!--android:layout_weight="1"-->
        <!--android:text=" : "-->
        <!--android:textSize="18sp" />-->

        <TextView
            android:id="@+id/proptTicketTotal"
            android:layout_width="40dp"
            android:layout_height="40dp"

            android:layout_weight="1"
            android:paddingRight="10dp"
            android:text="120"
            android:textAlignment="viewEnd"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/row6"
        android:layout_below="@+id/row5"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_marginBottom="5dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/proptDiscountedTicketLabel"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="2"
            android:text="Amount"
            android:textSize="22dp" />

        <!--<TextView-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="40dp"-->
        <!--android:layout_weight="1"-->
        <!--android:text=" : "-->
        <!--android:textSize="18sp" />-->

        <TextView
            android:id="@+id/proptDiscountedTicketTotal"
            android:layout_width="40dp"
            android:layout_height="40dp"

            android:layout_weight="1"
            android:paddingRight="10dp"
            android:text="120"
            android:textAlignment="textEnd"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/row7"
        android:layout_below="@+id/row6"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_marginBottom="12dp"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btnCanclePrint"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="#f05924"
            android:text="@string/cancle"
            android:layout_weight="1"
            android:textColor="@android:color/white" />

        <Button
            android:id="@+id/btnAllowPrint"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@drawable/btn_backl"
            android:text="@string/print"
            android:textColor="@android:color/white" />
    </LinearLayout>

</RelativeLayout>